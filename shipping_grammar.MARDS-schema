## Worksteps
##
## #######################

template shipping_workstep_grammar
    search function
        match 'package'
            extend package

template package
    describe en 
        title "pack materials for shipping"
    name fill
        value
            required
            type radio_select
                choice "starch_peanuts"
                choice preform_styro
        describe en
            title "material to fill around shipped parts"


